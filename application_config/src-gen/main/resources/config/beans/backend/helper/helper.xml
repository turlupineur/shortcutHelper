<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id = "PropertyHelper" class = "shortcutHelper.helper.propertyHelper.PropertyHelperImpl" parent="PropertyHelperDef">
</bean>
<bean id = "PropertyHelperDef" abstract="true">
</bean>
<bean id = "ShortcutHelper" class = "shortcutHelper.helper.shortcutHelper.ShortcutHelperImpl" parent="ShortcutHelperDef">
</bean>
<bean id = "ShortcutHelperDef" abstract="true">
</bean>
<bean id = "BeanHelper" class = "shortcutHelper.helper.beanHelper.BeanHelperImpl" parent="BeanHelperDef">
</bean>
<bean id = "BeanHelperDef" abstract="true">
</bean>
   
</beans>
